<quiz>
  <!-- MATE 3001 - Quiz 01: Conjuntos y Valor Absoluto -->
  <!-- Question Bank for Moodle XML Format -->
  
  <question type="category">
    <category>
      <text>$course$/MATE3001/Quiz01</text>
